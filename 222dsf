[32m+[m[32m  <!-- ==================== Default Welcome File List ===================== -->[m
[32m+[m[32m  <!-- When a request URI refers to a directory, the default servlet looks  -->[m
[32m+[m[32m  <!-- for a "welcome file" within that directory and, if present,          -->[m
[32m+[m[32m  <!-- to the corresponding resource URI for display.  If no welcome file   -->[m
[32m+[m[32m  <!-- is present, the default servlet either serves a directory listing,   -->[m
[32m+[m[32m  <!-- or returns a 404 status, depending on how it is configured.          -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!-- If you define welcome files in your own application's web.xml        -->[m
[32m+[m[32m  <!-- deployment descriptor, that list *replaces* the list configured      -->[m
[32m+[m[32m  <!-- here, so be sure that you include any of the default values that     -->[m
[32m+[m[32m  <!-- you wish to include.                                                 -->[m
[32m+[m
[32m+[m[32m    <welcome-file-list>[m
[32m+[m[32m        <welcome-file>index.html</welcome-file>[m
[32m+[m[32m        <welcome-file>index.htm</welcome-file>[m
[32m+[m[32m        <welcome-file>index.jsp</welcome-file>[m
[32m+[m[32m    </welcome-file-list>[m
[32m+[m
[32m+[m[32m</web-app>[m
