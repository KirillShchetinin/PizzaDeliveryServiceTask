[32m+[m
[32m+[m[32m  <!-- NOTE: An SSI Filter is also available as an alternative SSI          -->[m
[32m+[m[32m  <!-- implementation. Use either the Servlet or the Filter but NOT both.   -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!-- Server Side Includes processing servlet, which processes SSI         -->[m
[32m+[m[32m  <!-- directives in HTML pages consistent with similar support in web      -->[m
[32m+[m[32m  <!-- servers like Apache.  Traditionally, this servlet is mapped to the   -->[m
[32m+[m[32m  <!-- URL pattern "*.shtml".  This servlet supports the following          -->[m
[32m+[m[32m  <!-- initialization parameters (default values are in square brackets):   -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   buffered            Should output from this servlet be buffered?   -->[m
[32m+[m[32m  <!--                       (0=false, 1=true)  [0]                         -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   debug               Debugging detail level for messages logged     -->[m
[32m+[m[32m  <!--                       by this servlet.  [0]                          -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   expires             The number of seconds before a page with SSI   -->[m
[32m+[m[32m  <!--                       directives will expire.  [No default]          -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   isVirtualWebappRelative                                            -->[m
[32m+[m[32m  <!--                       Should "virtual" paths be interpreted as       -->[m
[32m+[m[32m  <!--                       relative to the context root, instead of       -->[m
[32m+[m[32m  <!--                       the server root?  (0=false, 1=true) [0]        -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   inputEncoding       The encoding to assume for SSI resources if    -->[m
[32m+[m[32m  <!--                       one is not available from the resource.        -->[m
[32m+[m[32m  <!--                       [Platform default]                             -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   outputEncoding      The encoding to use for the page that results  -->[m
[32m+[m[32m  <!--                       from the SSI processing. [UTF-8]               -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!-- IMPORTANT: To use the SSI servlet, you also need to rename the       -->[m
[32m+[m[32m  <!--            $CATALINA_HOME/server/lib/servlets-ssi.renametojar file   -->[m
[32m+[m[32m  <!--            to $CATALINA_HOME/server/lib/servlets-ssi.jar             -->[m
[32m+[m
[32m+[m[32m<!--[m
[32m+[m[32m    <servlet>[m
[32m+[m[32m        <servlet-name>ssi</servlet-name>[m
[32m+[m[32m        <servlet-class>[m
[32m+[m[32m          org.apache.catalina.ssi.SSIServlet[m
[32m+[m[32m        </servlet-class>[m
[32m+[m[32m        <init-param>[m
[32m+[m[32m          <param-name>buffered</param-name>[m
[32m+[m[32m          <param-value>1</param-value>[m
[32m+[m[32m        </init-param>[m
[32m+[m[32m        <init-param>[m
[32m+[m[32m          <param-name>debug</param-name>[m
[32m+[m[32m          <param-value>0</param-value>[m
[32m+[m[32m        </init-param>[m
[32m+[m[32m        <init-param>[m
[32m+[m[32m          <param-name>expires</param-name>[m
[32m+[m[32m          <param-value>666</param-value>[m
[32m+[m[32m        </init-param>[m
[32m+[m[32m        <init-param>[m
[32m+[m[32m          <param-name>isVirtualWebappRelative</param-name>[m
[32m+[m[32m          <param-value>0</param-value>[m
[32m+[m[32m        </init-param>[m
[32m+[m[32m        <load-on-startup>4</load-on-startup>[m
[32m+[m[32m    </servlet>[m
[32m+[m[32m-->[m
[32m+[m
[32m+[m
[32m+[m[32m  <!-- Common Gateway Includes (CGI) processing servlet, which supports     -->[m
[32m+[m[32m  <!-- execution of external applications that conform to the CGI spec      -->[m
[32m+[m[32m  <!-- requirements.  Typically, this servlet is mapped to the URL pattern  -->[m
[32m+[m[32m  <!-- "/cgi-bin/*", which means that any CGI applications that are         -->[m
[32m+[m[32m  <!-- executed must be present within the web application.  This servlet   -->[m
[32m+[m[32m  <!-- supports the following initialization parameters (default values     -->[m
[32m+[m[32m  <!-- are in square brackets):                                             -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   cgiPathPrefix        The CGI search path will start at             -->[m
[32m+[m[32m  <!--                        webAppRootDir + File.separator + this prefix. -->[m
[32m+[m[32m  <!--                        [WEB-INF/cgi]                                 -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   debug                Debugging detail level for messages logged    -->[m
[32m+[m[32m  <!--                        by this servlet.  [0]                         -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   executable           Name of the exectuable used to run the        -->[m
[32m+[m[32m  <!--                        script. [perl]                                -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   parameterEncoding    Name of parameter encoding to be used with    -->[m
[32m+[m[32m  <!--                        CGI servlet.                                  -->[m
[32m+[m[32m  <!--                        [System.getProperty("file.encoding","UTF-8")] -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   passShellEnvironment Should the shell environment variables (if    -->[m
[32m+[m[32m  <!--                        any) be passed to the CGI script? [false]     -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!-- IMPORTANT: To use the CGI servlet, you also need to rename the       -->[m
[32m+[m[32m  <!--            $CATALINA_HOME/server/lib/servlets-cgi.renametojar file   -->[m
[32m+[m[32m  <!--            to $CATALINA_HOME/server/lib/servlets-cgi.jar             -->[m
[32m+[m
[32m+[m[32m<!--[m
[32m+[m[32m    <servlet>[m
[32m+[m[32m        <servlet-name>cgi</servlet-name>[m
[32m+[m[32m        <servlet-class>org.apache.catalina.servlets.CGIServlet</servlet-class>[m
[32m+[m[32m        <init-param>[m
[32m+[m[32m          <param-name>debug</param-name>[m
[32m+[m[32m          <param-value>0</param-value>[m
[32m+[m[32m        </init-param>[m
[32m+[m[32m        <init-param>[m
[32m+[m[32m          <param-name>cgiPathPrefix</param-name>[m
[32m+[m[32m          <param-value>WEB-INF/cgi</param-value>[m
[32m+[m[32m        </init-param>[m
[32m+[m[32m         <load-on-startup>5</load-on-startup>[m
[32m+[m[32m    </servlet>[m
[32m+[m[32m-->[m
[32m+[m
[32m+[m
[32m+[m[32m  <!-- ================ Built In Servlet Mappings ========================= -->[m
[32m+[m
[32m+[m
[32m+[m[32m  <!-- The servlet mappings for the built in servlets defined above.  Note  -->[m
[32m+[m[32m  <!-- that, by default, the CGI and SSI servlets are *not* mapped.  You    -->[m
[32m+[m[32m  <!-- must uncomment these mappings (or add them to your application's own -->[m
[32m+[m[32m  <!-- web.xml deployment descriptor) to enable these services              -->[m
[32m+[m
[32m+[m[32m    <!-- The mapping for the default servlet -->[m
[32m+[m[32m    <servlet-mapping>[m
[32m+[m[32m        <servlet-name>default</servlet-name>[m
[32m+[m[32m        <url-pattern>/</url-pattern>[m
[32m+[m[32m    </servlet-mapping>[m
[32m+[m
[32m+[m[32m    <!-- The mapping for the invoker servlet -->[m
[32m+[m[32m<!--[m
[32m+[m[32m    <servlet-mapping>[m
[32m+[m[32m        <servlet-name>invoker</servlet-name>[m
[32m+[m[32m        <url-pattern>/servlet/*</url-pattern>[m
[32m+[m[32m    </servlet-mapping>[m
[32m+[m[32m-->[m
[32m+[m
[32m+[m[32m    <!-- The mapping for the JSP servlet -->[m
[32m+[m[32m    <servlet-mapping>[m
[32m+[m[32m        <servlet-name>jsp</servlet-name>[m
[32m+[m[32m        <url-pattern>*.jsp</url-pattern>[m
[32m+[m[32m    </servlet-mapping>[m
[32m+[m
[32m+[m[32m    <servlet-mapping>[m
[32m+[m[32m        <servlet-name>jsp</servlet-name>[m
[32m+[m[32m        <url-pattern>*.jspx</url-pattern>[m
[32m+[m[32m    </servlet-mapping>[m
[32m+[m
[32m+[m[32m    <!-- The mapping for the SSI servlet -->[m
[32m+[m[32m<!--[m
[32m+[m[32m    <servlet-mapping>[m
[32m+[m[32m        <servlet-name>ssi</servlet-name>[m
[32m+[m[32m        <url-pattern>*.shtml</url-pattern>[m
[32m+[m[32m    </servlet-mapping>[m
[32m+[m[32m-->[m
[32m+[m
[32m+[m[32m    <!-- The mapping for the CGI Gateway servlet -->[m
[32m+[m
[32m+[m[32m<!--[m
[32m+[m[32m    <servlet-mapping>[m
[32m+[m[32m        <servlet-name>cgi</servlet-name>[m
[32m+[m[32m        <url-pattern>/cgi-bin/*</url-pattern>[m
[32m+[m[32m    </servlet-mapping>[m
[32m+[m[32m-->[m
[32m+[m
[32m+[m
[32m+[m[32m  <!-- ================== Built In Filter Definitions ===================== -->[m
[32m+[m
[32m+[m[32m  <!-- NOTE: An SSI Servlet is also available as an alternative SSI         -->[m
[32m+[m[32m  <!-- implementation. Use either the Servlet or the Filter but NOT both.   -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!-- Server Side Includes processing filter, which processes SSI          -->[m
[32m+[m[32m  <!-- directives in HTML pages consistent with similar support in web      -->[m
[32m+[m[32m  <!-- servers like Apache.  Traditionally, this filter is mapped to the    -->[m
[32m+[m[32m  <!-- URL pattern "*.shtml", though it can be mapped to "*" as it will     -->[m
[32m+[m[32m  <!-- selectively enable/disable SSI processing based on mime types. For   -->[m
[32m+[m[32m  <!-- this to work you will need to uncomment the .shtml mime type         -->[m
[32m+[m[32m  <!-- definition towards the bottom of this file.                          -->[m
[32m+[m[32m  <!-- The contentType init param allows you to apply SSI processing to JSP -->[m
[32m+[m[32m  <!-- pages, javascript, or any other content you wish.  This filter       -->[m
[32m+[m[32m  <!-- supports the following initialization parameters (default values are -->[m
[32m+[m[32m  <!-- in square brackets):                                                 -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   contentType         A regex pattern that must be matched before    -->[m
[32m+[m[32m  <!--                       SSI processing is applied.                     -->[m
[32m+[m[32m  <!--                       [text/x-server-parsed-html(;.*)?]              -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   debug               Debugging detail level for messages logged     -->[m
[32m+[m[32m  <!--                       by this servlet.  [0]                          -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   expires             The number of seconds before a page with SSI   -->[m
[32m+[m[32m  <!--                       directives will expire.  [No default]          -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--   isVirtualWebappRelative                                            -->[m
[32m+[m[32m  <!--                       Should "virtual" paths be interpreted as       -->[m
[32m+[m[32m  <!--                       relative to the context root, instead of       -->[m
[32m+[m[32m  <!--                       the server root?  (0=false, 1=true) [0]        -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!--                                                                      -->[m
[32m+[m[32m  <!-- IMPORTANT: To use the SSI filter, you also need to rename the        -->[m
[32m+[m[32m  <!--            $CATALINA_HOME/server/lib/servlets-ssi.renametojar file   -->[m
[32m+[m[32m  <!--            to $CATALINA_HOME/server/lib/servlets-ssi.jar             -->[m
[32m+[m
[32m+[m[32m<!--[m
[32m+[m[32m    <filter>[m
[32m+[m[32m        <filter-name>ssi</filter-name>[m
[32m+[m[32m        <filter-class>[m
[32m+[m[32m          org.apache.catalina.ssi.SSIFilter[m
[32m+[m[32m        </filter-class>[m
[32m+[m[32m        <init-param>[m
[32m+[m[32m          <param-name>contentType</param-name>[m
[32m+[m[32m          <param-value>text/x-server-parsed-html(;.*)?</param-value>[m
[32m+[m[32m        </init-param>[m
[32m+[m[32m        <init-param>[m
[32m+[m[32m          <param-name>debug</param-name>[m
[32m+[m[32m          <param-value>0</param-value>[m
[32m+[m[32m        </init-param>[m
[32m+[m[32m        <init-param>[m
[32m+[m[32m          <param-name>expires</param-name>[m
[32m+[m[32m          <param-value>666</param-value>[m
[32m+[m[32m        </init-param>[m
[32m+[m[32m        <init-param>[m
[32m+[m[32m          <param-name>isVirtualWebappRelative</param-name>[m
[32m+[m[32m          <param-value>0</param-value>[m
[32m+[m[32m        </init-param>[m
[32m+[m[32m    </filter>[m
[32m+[m[32m-->[m
[32m+[m
[32m+[m
[32m+[m[32m  <!-- ==================== Built In Filter Mappings ====================== -->[m
[32m+[m
[32m+[m[32m  <!-- The mapping for the SSI Filter -->[m
[32m+[m[32m<!--[m
[32m+[m[32m    <filter-mapping>[m
[32m+[m[32m        <filter-name>ssi</filter-name>[m
[32m+[m[32m        <url-pattern>*.shtml</url-pattern>[m
[32m+[m[32m    </filter-mapping>[m
[32m+[m[32m-->[m
[32m+[m
[32m+[m
[32m+[m[32m  <!-- ==================== Default Session Configuration ================= -->[m
[32m+[m[32m  <!-- You can set the default session timeout (in minutes) for all newly   -->[m
[32m+[m[32m  <!-- created sessions by modifying the value below.                       -->[m
[32m+[m
[32m+[m[32m    <session-config>[m
[32m+[m[32m        <session-timeout>30</session-timeout>[m
[32m+[m[32m    </session-config>[m
[32m+[m
[32m+[m
[32m+[m[32m  <!-- ===================== Default MIME Type Mappings =================== -->[m
[32m+[m[32m  <!-- When serving static resources, Tomcat will automatically generate    -->[m
[32m+[m[32m  <!-- a "Content-Type" header based on the resource's filename extension,  -->[m
[32m+[m[32m  <!-- based on these mappings.  Additional mappings can be added here (to  -->[m
[32m+[m[32m  <!-- apply to all web applications), or in your own application's web.xml -->[m
[32m+[m[32m  <!-- deployment descriptor.                                               -->[m
[32m+[m
[32m+[m[32m    <mime-mapping>[m
[32m+[m[32m        <extension>abs</extension>[m
[32m+[m[32m        <mime-type>audio/x-mpeg</mime-type>[m
[32m+[m[32m    </mime-mapping>[m
